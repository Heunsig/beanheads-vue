{
  "name": "beanheads-vue",
  "version": "1.2.1",
  "license": "MIT",
  "author": "Heunsig",
  "description": "Easily generate avatars for your Vue and Nuxt projects with Beanheads Vue.",
  "private": false,
  "type": "module",
  "main": "./dist/beanheads-vue.umd.cjs",
  "module": "./dist/beanheads-vue.js",
  "exports":{
    ".": {
      "import": "./dist/beanheads-vue.js",
      "require": "./dist/beanheads-vue.umd.cjs"
    }
  },
  "homepage": "https://beanheads-vue.heunsig.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Heunsig/beanheads-vue"
  },
  "scripts": {
    "dev": "vite build --watch",
    "dev-core": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "npm-run-all2": "^7.0.1",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-tsc": "^2.1.10"
  }
}
